  //speakerid/conversationid 93

  speed       12

  seesound    inqsee
  deathsound  inqdth
  activesound inqact

  radius      40.0
  height      110.0

  cflags      SOLID|SHOOTABLE|DROPOFF|NOBLOOD|COUNTKILL // SVE_TODO: MF2_DRAWBILLBOARD

  states
  @"
  Spawn:
    ROB3 AB 10 A_Look
    loop
  See:
    ROB3 B 3 // STRIFE_TODO: A_InqChase
    ROB3 B 3
    ROB3 CCCDD 4 A_Chase
    ROB3 E 3 // STRIFE_TODO: A_InqChase
    ROB3 E 3 // STRIFE_TODO: A_InqFlyCheck
    loop
  Missile:
    ROB3 A 2 // STRIFE_TODO: A_InqFlyCheck
    ROB3 F 6 A_FaceTarget
    ROB3 G 8 bright // STRIFE_TODO: A_ReaverAttack
    ROB3 G 8 // STRIFE_TODO: A_ReaverAttack
    loop
  Grenade:
    ROB3 K 12 A_FaceTarget
    ROB3 J 6 bright // STRIFE_TODO: A_InqGrenade
    ROB3 K 12
    goto See
  Fly:
    ROB3 H 8 bright // STRIFE_TODO: A_InqTakeOff
    ROB3 IH 4 bright // STRIFE_TODO: A_InqFly
    goto Fly+1
  Death:
    ROB3 L 4 // STRIFE_TODO: A_BodyParts
    ROB3 M 4 A_Scream
    ROB3 N 4 // STRIFE_TODO: A_BodyParts
    ROB3 O 4 bright // STRIFE_TODO: A_DeathExplode1
    ROB3 P 4 bright // STRIFE_TODO: A_BodyParts
    ROB3 Q 4 bright A_Fall
    ROB3 RSTUV 4 // STRIFE_TODO: A_BodyParts
    ROB3 W 4 bright // STRIFE_TODO: A_DeathExplode1
    ROB3 XY 4 bright // STRIFE_TODO: A_BodyParts
    ROB3 "Z[" 4 // STRIFE_TODO: A_BodyParts
    ROB3 "\" 3 // STRIFE_TODO: A_BodyParts
    ROB3 "]" 3 bright // STRIFE_TODO: A_DeathExplode1
    RBB3 A 3 bright // STRIFE_TODO: A_InqTossArm
    RBB3 B 3 bright // STRIFE_TODO: A_BodyParts
    RBB3 CD 3 // STRIFE_TODO: A_BodyParts
    RBB3 E -1 // STRIFE_TODO: Strife's A_BossDeath
    stop
  "@
}

